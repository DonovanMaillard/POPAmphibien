  "keep": [
    "observers",
    "visit_date_min",
    "pluviosite",
    "couverture_nuageuse",
    "vent",
    "methode_de_prospection",
    "presence_amphibien",
    "presence_poisson",
    "comments"
  ],
  "specific": {
    "num_passage": {
      "type_widget": "select",
      "required": true,
      "attribut_label": "Numéro de passage",
      "values": ["1", "2", "3"]
    },
    "id_base_site": {
      "type_widget": "site",
      "required": true,
      "attribut_label": "Site"
    },
    "pluviosite": {
      "type_widget": "nomenclature",
      "attribut_label": "Pluviosité",
      "code_nomenclature_type": "PLUVIOSITE",
	    "required": false,
      "type_util": "nomenclature"
    },
    "couverture_nuageuse": {
      "type_widget": "nomenclature",
      "attribut_label": "Couverture nuageuse",
      "code_nomenclature_type": "COUVERTURE_NUAGEUSE",
	    "required": false,
      "type_util": "nomenclature"
    },
    "vent": {
      "type_widget": "nomenclature",
      "attribut_label": "Vent",
      "code_nomenclature_type": "VENT",
	    "required": false,
      "type_util": "nomenclature"    
    },
    "methode_de_prospection": {
      "type_widget": "nomenclature",
      "attribut_label": "Méthode de prospection",
      "code_nomenclature_type": "METHODE_PROSPECTION",
	    "required": false,
      "type_util": "nomenclature"
        },
    "comments": {
      "type_widget": "textarea",
      "attribut_label": "Commentaire"
    },
    "presence_amphibien": {
      "type_widget": "radio",
      "attribut_label": "Présence d’amphibiens",
      "values": ["Oui", "Non"]
    },
    "presence_poisson": {
      "type_widget": "radio",
      "attribut_label": "Présence de poissons",
      "values": ["Oui", "Non"]
    },
    "id_dataset": {
     
      "type_widget": "datalist",
      "attribut_label": "Jeu de données",
      "type_util": "dataset",
      "api": "meta/datasets",
      "application": "GeoNature",
      "keyValue": "id_dataset",
      "keyLabel": "dataset_shortname",
      "params": {
        "orderby": "dataset_name",
        "module_code": "__MODULE.MODULE_CODE"
      },
      "data_path": "data",
      "required": true
    },  
     "visit_date_min": {
      "type_widget": "date",
      "attribut_label": "Date du passage",
      "required": true
    },
    "nb_observations": {
      "attribut_label": "Nb. d'observations"
    }
  }
